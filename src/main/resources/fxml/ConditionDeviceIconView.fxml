<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_CENTER" maxHeight="60.0" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" style="-fx-background-color: #404040; -fx-background-radius: 5px 5px 5px 5px; -fx-border-color: #1c1c1c; -fx-border-radius: 5px 5px 5px 5px;" type="VBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane prefHeight="35.0" prefWidth="35.0">
         <children>
            <ImageView fx:id="iconImageView" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="removeConditionDeviceBtn" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: transparent;" StackPane.alignment="TOP_RIGHT">
               <graphic>
                  <ImageView fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/cancel.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </StackPane>
      <Label fx:id="nameIconImageView" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="50.0" text="Label" textAlignment="CENTER" textFill="#777677" textOverrun="CENTER_WORD_ELLIPSIS">
         <font>
            <Font name="Segoe UI Bold" size="10.0" />
         </font>
      </Label>
   </children>
</fx:root>
