<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root id="aa" style="-fx-background-color: #272B35;" stylesheets="@../css/NewDevicePanelView.css" type="SplitPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <VBox id="bb" style="-fx-background-color: #272B35;">
         <children>
            <Button id="right_button" fx:id="deviceBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Device">
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Button>
            <Button id="right_button" fx:id="diagramBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Diagram" />
            <Button id="right_button" fx:id="configBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Configure Device" />
            <Button id="right_button" fx:id="generateBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Generate" />
         </children>
      </VBox>
   </items>
</fx:root>
