<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<?import io.makerplayground.ui.control.AutoResizeTextField?>

<?import javafx.scene.layout.Pane?>
<fx:root alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
         minWidth="-Infinity" spacing="10.0" type="HBox" stylesheets="@../css/ProjectDeviceListCell.css"
         xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="deviceIcon" fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true"
                   HBox.hgrow="NEVER"/>
        <AutoResizeTextField id="nameTextField" fx:id="nameTextField" maxWidth="100" HBox.hgrow="ALWAYS"/>
        <Pane maxWidth="1.7976931348623157E308" prefHeight="20.0" prefWidth="20.0" HBox.hgrow="ALWAYS" />
        <Button fx:id="removeButton" mnemonicParsing="false" text="X" HBox.hgrow="NEVER"/>
    </children>
</fx:root>
