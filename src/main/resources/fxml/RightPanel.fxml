<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<fx:root stylesheets="@../css/RightPanel.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.111"
         xmlns:fx="http://javafx.com/fxml/1" maxWidth="330">
    <VBox id="vBox" fx:id="vBox" spacing="2" VBox.vgrow="NEVER">
        <padding>
            <Insets top="20" right="20" bottom="20" left="20"/>
        </padding>
        <Button id="button" fx:id="configureBtn" text="Configure Device" maxWidth="Infinity"
                onAction="#handleConfigureBtn"/>
        <Button id="button" fx:id="generateBtn" text="Generate Project" maxWidth="Infinity"
                onAction="#handleGenerateBtn"/>
        <Button id="button" fx:id="uploadBtn" text="Upload" maxWidth="Infinity" onAction="#handleUploadBtn"/>
    </VBox>
</fx:root>
