<fx:root fx:id="beginVBox" alignment="TOP_RIGHT" stylesheets="@../../../css/canvas/node/BeginScene.css" type="VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" style="-fx-cursor: move;">
   <children>
      <Button fx:id="removeBeginBtn" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-cursor:hand;">
         <graphic>
            <ImageView fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../icons/cancel-2.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <HBox fx:id="labelPane" alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <Label fx:id="labelHBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="95.0" text="Begin" textAlignment="CENTER" />
         <Arc fx:id="outPort" length="180.0" radiusX="10.0" radiusY="10.0" startAngle="270.0" strokeType="INSIDE" strokeWidth="0.0" />
      </HBox>
   </children>
</fx:root>
