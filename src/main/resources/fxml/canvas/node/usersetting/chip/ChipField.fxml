<fx:root spacing="5" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <ScrollPane id="scrollPane" fx:id="scrollPane" hbarPolicy="NEVER" vbarPolicy="NEVER" minHeight="27.0" minWidth="75"
                stylesheets="@../../../../../css/canvas/node/usersetting/chip/ChipField.css" >
        <content>
            <StackPane>
                <children>
                    <HBox fx:id="mainPane" alignment="CENTER_LEFT" minHeight="25.0" >
                        <children>
                            <Rectangle fx:id="cursor" fill="#333333" height="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="1.0">
                                <HBox.margin>
                                    <Insets left="2.0" right="2.0" top="2.0" />
                                </HBox.margin>
                            </Rectangle>
                        </children>
                    </HBox>
                    <VBox fx:id="displayModePane" alignment="CENTER_LEFT" minHeight="25.0" >
                        <children>
                            <TextFlow fx:id="displayModeTextFlow" maxWidth="250.0" >
                                <padding>
                                    <Insets left="8.0" />
                                </padding>
                            </TextFlow>
                        </children>
                    </VBox>
                </children>
            </StackPane>
        </content>
    </ScrollPane>
    <GridPane fx:id="chipSelectorPane" alignment="CENTER" hgap="5.0" vgap="5.0">
        <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints />
            <RowConstraints />
        </rowConstraints>
        <children>
            <StackPane fx:id="numberAnimationChip" GridPane.columnIndex="0">
                <children>
                    <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#21b0fd" height="25.0" width="60.0" />
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="Number" />
                </children>
            </StackPane>
            <StackPane fx:id="stringAnimationChip" GridPane.columnIndex="0" GridPane.rowIndex="1">
                <children>
                    <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#c76e56" height="25.0" width="60.0" />
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="String" />
                </children>
            </StackPane>
            <StackPane fx:id="numberChip" GridPane.columnIndex="1">
                <children>
                    <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#2673fd" height="25.0" width="60.0" />
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="Number" />
                </children>
            </StackPane>
            <StackPane fx:id="stringChip" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <children>
                    <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#c75450" height="25.0" width="60.0" />
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="String" />
                </children>
            </StackPane>
            <StackPane fx:id="valueChip" GridPane.columnIndex="2">
                <children>
                    <Rectangle arcHeight="25.0" arcWidth="25.0" fill="#9129d7" height="25.0" width="60.0" />
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="Value" />
                </children>
            </StackPane>
            <StackPane fx:id="plusChip" GridPane.columnIndex="3">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="+" />
                </children>
            </StackPane>
            <StackPane fx:id="multiplyChip" GridPane.columnIndex="3" GridPane.rowIndex="1">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="x" />
                </children>
            </StackPane>
            <StackPane fx:id="minusChip" GridPane.columnIndex="4">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="-" />
                </children>
            </StackPane>
            <StackPane fx:id="divideChip" GridPane.columnIndex="4" GridPane.rowIndex="1">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="/" />
                </children>
            </StackPane>
            <StackPane fx:id="moduloChip" GridPane.columnIndex="5">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="\%" />
                </children>
            </StackPane>
            <StackPane fx:id="divideIntChip" GridPane.columnIndex="5" GridPane.rowIndex="1">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="35.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="35.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="//" />
                </children>
            </StackPane>
            <StackPane fx:id="openParenthesisChip" GridPane.columnIndex="6">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="10.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="false" x="0.0" y="10.0" />
                            <LineTo x="0.0" y="15.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="false" x="10.0" y="25.0" />
                            <LineTo x="25.0" y="25.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="15.0" y="15.0" />
                            <LineTo x="15.0" y="10.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="25.0" y="0.0" />
                            <ClosePath />
                        </elements>
                        <StackPane.margin>
                            <Insets />
                        </StackPane.margin>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text="(">
                        <StackPane.margin>
                            <Insets right="7.5" />
                        </StackPane.margin>
                    </Text>
                </children>
            </StackPane>
            <StackPane fx:id="closeParenthesisChip" GridPane.columnIndex="6" GridPane.rowIndex="1">
                <children>
                    <Path fill="#ff8b01" stroke="#ffffff">
                        <elements>
                            <MoveTo x="15.0" y="0.0" />
                            <LineTo x="0.0" y="0.0" />
                            <ArcTo XAxisRotation="90.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="10.0" y="10.0" />
                            <LineTo x="10.0" y="15.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="true" x="0.0" y="25.0" />
                            <LineTo x="15.0" y="25.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="false" x="25.0" y="15.0" />
                            <LineTo x="25.0" y="10.0" />
                            <ArcTo XAxisRotation="0.0" largeArcFlag="false" radiusX="10.0" radiusY="10.0" sweepFlag="false" x="15.0" y="0.0" />
                            <ClosePath />
                        </elements>
                    </Path>
                    <Text fill="#ffffff" strokeType="OUTSIDE" strokeWidth="0.0" text=")">
                        <StackPane.margin>
                            <Insets left="7.5" />
                        </StackPane.margin>
                    </Text>
                </children>
            </StackPane>
        </children>
    </GridPane>
</fx:root>
