<fx:root maxHeight="-Infinity" prefWidth="400.0" style="-fx-background-color: #edeef3;"
         stylesheets="@../../../css/dialog/tutorial/TutorialView.css" type="AnchorPane"
         xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <Button fx:id="prevBtn" layoutY="135.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" text="&lt;" AnchorPane.bottomAnchor="310.0"
           AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="310.0"/>
