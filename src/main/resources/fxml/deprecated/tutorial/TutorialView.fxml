              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"
              AnchorPane.topAnchor="0.0"/>
   <Button fx:id="nextBtn" layoutY="135.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false"
           prefHeight="30.0" text="&gt;" AnchorPane.bottomAnchor="310.0" AnchorPane.rightAnchor="0.0"
           AnchorPane.topAnchor="310.0"/>
   <Button id="cancelBtn" fx:id="cancelBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" AnchorPane.rightAnchor="10.0"
           AnchorPane.topAnchor="10.0">
      <font>
         <Font name="System Bold" size="12.0"/>
      </font>
   </Button>
