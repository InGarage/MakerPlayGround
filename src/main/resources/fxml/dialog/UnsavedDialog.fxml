<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="11.0" prefWidth="200.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="300.0" />
            <Button mnemonicParsing="false" />
         </children>
      </HBox>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <HBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" text="Save" />
            <Button mnemonicParsing="false" text="Don't save" />
            <Button mnemonicParsing="false" text="Cancel" />
         </children>
      </HBox>
   </children>
</VBox>
