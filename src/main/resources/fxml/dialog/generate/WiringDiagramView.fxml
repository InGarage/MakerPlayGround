<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<fx:root stylesheets="@../../../css/dialog/generate/GenerateView.css" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane HBox.hgrow="ALWAYS">
            <children>
                <ScrollPane id="diagramScrollPane" fx:id="diagramScrollPane" pannable="true" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" />
                <HBox alignment="CENTER_LEFT" layoutY="20.0" spacing="5.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
                    <children>
                        <Label text="Port" />
                        <ComboBox id="interactivePortCombobox" fx:id="portCombobox" />
                        <Button id="startInteractiveButton" fx:id="startInteractiveModeButton" text="Start Interactive Mode" />
                    </children>
                </HBox>
                <VBox id="zoomControl" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0">
                    <Button id="zoomInButton" fx:id="zoomInButton" prefHeight="25" prefWidth="25" />
                    <Button id="zoomOutButton" fx:id="zoomOutButton" prefHeight="25" prefWidth="25" />
                    <Pane prefHeight="9" />
                    <Button id="zoomDefaultButton" fx:id="zoomDefaultButton" prefHeight="25" prefWidth="25" />
                </VBox>
            </children>
        </AnchorPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</fx:root>
